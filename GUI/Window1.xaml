<Window x:Class="GUI.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window1" Height="408.511" Width="455.319">
    <Grid>
        <Button x:Name="BT_Run" Content="Run" HorizontalAlignment="Left" Margin="175,34,0,0" VerticalAlignment="Top" Width="75" Click="BT_Run_Click"/>
        <ListView Name="Results" HorizontalAlignment="Left" Height="183" Margin="140,116,0,0" VerticalAlignment="Top" Width="133">
            <ListView.View>
                <GridView AllowsColumnReorder="True">
                    <GridView.Columns>
                        <GridViewColumn Header="Id"
                            DisplayMemberBinding="{Binding id}" Width="30" />             
                        <GridViewColumn Header="customersNr"
                            DisplayMemberBinding="{Binding customersNr}" Width="100" />
                    </GridView.Columns>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
